{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc --watch",
    "dev:app": "nodemon --config nodemon.json app dist/index.js",
    "dev:socket": "nodemon --config nodemon.json socket dist/socket.js",
    "dev:deleteWorker": "nodemon --config nodemon.json deleteWorker dist/workers/deleteWorker.js",
    "dev:sessionCleanUpWorker": "nodemon --config nodemon.json sessionCleanUpWorker dist/workers/sessionCleanUpWorker.js",
    "start:app": "node dist/index.js",
    "start:deleteWorker": "node dist/workers/deleteWorker.js",
    "start:sessionCleanUpWorker": "node dist/workers/sessionCleanUpWorker.js",
    "dev": "concurrently \"npm run watch-ts\" \"npm run dev:app\" \"npm run dev:socket\" \"npm run dev:deleteWorker\" \" npm run dev:sessionCleanUpWorker\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bull": "^3.15.9",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.0",
    "bull": "^4.16.5",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "http": "^0.0.1-security",
    "jsend": "^1.1.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.2",
    "morgan": "^1.10.0",
    "openai": "^4.77.0",
    "socket.io": "^4.8.1",
    "ts-node-dev": "^2.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@bull-board/api": "^6.6.1",
    "@bull-board/express": "^6.6.1",
    "@bull-board/ui": "^6.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^20.17.12",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2"
  }
}
